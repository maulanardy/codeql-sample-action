#include <openssl/bn.h>

static const long Cryptography_HAS_BN_FLAGS;

typedef ... BN_CTX;
typedef ... BN_MONT_CTX;
typedef ... BIGNUM;
typedef int... BN_ULONG;

#define BN_FLG_CONSTTIME ...

